<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<HBox prefHeight="617.0" prefWidth="842.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="utc.pokerut.client.ihmmain.controllers.WaitingRoomController">
   <fx:include source="leftPanel.fxml" />
   <Pane prefHeight="617.0" prefWidth="560.0">
      <children>
         <TableView fx:id="myTableView" layoutX="42.0" layoutY="28.0" prefHeight="483.0" prefWidth="484.0" style="-fx-background-color: #FFFFFF; -fx-border-color: #000000;">
            <columns>
               <TableColumn fx:id="username" style="-fx-background-color: #E3E3E3;-fx-alignment: center;" text="Nom d'utilisateur" />
               <TableColumn fx:id="status" style="-fx-background-color: #E3E3E3;-fx-alignment: center;" text="STATUT" />
            </columns>
            <columnResizePolicy>
               <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
         </TableView>
         <Button fx:id="launchGame" layoutX="411.0" layoutY="526.0" mnemonicParsing="false" onAction="#handleLaunchGameButtonAction" text="Lancer partie" />
         <Button fx:id="cancel" layoutX="273.0" layoutY="526.0" mnemonicParsing="false" onAction="#handleCancelButtonAction" text="Annuler" />
      </children>
   </Pane>
</HBox>
